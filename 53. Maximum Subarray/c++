class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        if(nums.empty()) return 0;
        
        int result = nums[0];
        vector<int> sums{nums[0]};
        
        for(int i = 1; i < nums.size(); i++){
            sums.push_back(max(sums[i-1]+nums[i], nums[i]));
            result = max(sums[i], result);
        }
        
        
        return result;
    }
};